    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@lit-labs/router": "0.1.3",
    "@lit/context": "^1.1.0",
    "@shoelace-style/shoelace": "2.15.0",
    "@tailwindcss/typography": "^0.5.9",
